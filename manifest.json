{
    "manifest_version": 3,
    "name": "LinkedIn Connection Scraper",
    "version": "1.0",
    "permissions": ["storage", "activeTab", "scripting", "tabs"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/*"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }
  
  